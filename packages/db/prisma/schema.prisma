
generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}


model userschema {
    id Int @id @default(autoincrement())
    firstname String
    lastname  String
    username  String @unique
    password String
    contentschema contentschema[]
    dataandtime    DateTime
}

model contentschema {
      id Int  @id  @default(autoincrement())
      userid Int  
      user userschema @relation(fields: [userid] , references: [id])
      Link  String
      title  String
      type String
      dataandtime   DateTime
}